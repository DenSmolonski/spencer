{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "$id": "mutable-entity",
  "allOf": [
    "immutable-entity#",
    {
      "properties": {
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    }
  ],
  "required": ["id", "createdAt", "updatedAt"]
}